local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/obeseinsect/roblox/main/Ui%20Libraries/Elerium.lua"))()

local window = library:AddWindow("Muki's private")
local mainTab = window:AddTab("farming")

local autoRepEnabled = false

mainTab:AddSwitch("Auto Rep", function(state)
    autoRepEnabled = state
end)

task.spawn(function()
    while true do
        if autoRepEnabled then
            pcall(function()
                muscleEvent:FireServer("rep", interactSeat)
            end)
        end
        task.wait(0.1)
    end
end)

mainTab:AddLabel("by Muki â€“ Elerium ready")
